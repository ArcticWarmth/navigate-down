{
  "name": "navigate-down",
  "displayName": "Navigate Down",
  "publisher": "ArcticWarmth",
  "description": "Add a reopen in folder to explorer content menu",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "icon": "img/logo.png",
  "main": "./extension.js",
  "repository": "https://github.com/ArcticWarmth/navigate-down",
  "contributes": {
    "menus": {
      "commandPalette": [
        {
          "command": "navigate-down.open",
          "when": "false"
        }
      ],
      "explorer/context": [
        {
          "command": "navigate-down.open",
          "when": "explorerResourceIsFolder",
          "group": "navigation@999"
        }
      ]
    },
    "commands": [
      {
        "command": "navigate-down.open",
        "title": "Reopen Code in folder"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^24.2.0",
    "@types/vscode": "^1.82.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^9.32.0",
    "mocha": "^11.7.1",
    "typescript": "^5.1.6"
  }
}
